<h1>Sign In</h1>

<div class="row">
  <div class="span6 offset3">
<%= flash[:error] %>
<%= form_for(:session, url: sessions_path) do |f| %>
  

<div>

    <%= f.label :email %>
    <%= f.text_field :email %>

</div>

<div>
    <%= f.label :password %>
    <%= f.text_field :password %>

</div>  

  <div class="actions">
    <%= f.submit "Press ENTER Here!" %> 
  </div>
  <p>New user? <%= link_to "Sign up now!", signup_path %></p>
<% end %>